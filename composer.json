{
    "name": "martinmdev/php-url",
    "license": "MIT",
    "description": "PHP Url",
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "martinmdev/php-debug": "dev-dev"
    },
    "require": {
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Martinm\\Url\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Martinm\\Test\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload"
        ],
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    },
    "bin": [
        "bin/console"
    ]
}
